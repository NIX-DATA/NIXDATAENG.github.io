    "import zipfile\n",
    "files = ['file1.txt','file2.txt']\n",
    "print(files)\n",
    "\n",
    "with zipfile.ZipFile('pycl.zip','w') as zipf:\n",
    "    for file in files:\n",
    "        zipf.write(file)\n",
    "\n",
    "print(\"ZIP file Created!\")"
